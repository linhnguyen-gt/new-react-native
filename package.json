{
  "name": "NewReactNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --mode=developmentDebug --appId=com.newreactnative.dev",
    "android:stg": "cd android && ENVFILE=.env.staging && ./gradlew clean && cd .. && react-native run-android --mode=stagingDebug --appId=com.newreactnative.stg",
    "android:dev": "cd android && ENVFILE=.env.development && ./gradlew clean && cd .. && react-native run-android --mode=developmentDebug --appId=com.newreactnative.dev",
    "android:pro": "cd android && ENVFILE=.env.production && ./gradlew clean && cd .. && react-native run-android --mode=productionDebug --appId=com.newreactnative.production",
    "ios": "react-native run-ios",
    "ios:stg": "react-native run-ios --scheme Staging --mode Staging.Debug",
    "ios:dev": "react-native run-ios --scheme Dev --mode Dev.Debug",
    "lint": "eslint .",
    "start": "react-native start",
    "lint:fix": "eslint . --fix --ext .ts,.tsx",
    "test": "jest",
    "adb:reactotron": "adb reverse tcp:9090 tcp:9090"
  },
  "dependencies": {
    "@gluestack-style/react": "^1.0.57",
    "@gluestack-ui/config": "^1.1.20",
    "@gluestack-ui/image": "^0.1.10",
    "@gluestack-ui/nativewind-utils": "^1.0.25",
    "@gluestack-ui/themed": "^1.1.54",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@reduxjs/toolkit": "^2.2.8",
    "axios": "1.7.7",
    "lodash": "^4.17.21",
    "nativewind": "^4.1.19",
    "react": "18.3.1",
    "react-native": "0.75.4",
    "react-native-config": "^1.5.3",
    "react-native-reanimated": "^3.15.4",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-svg": "15.7.1",
    "react-redux": "^9.1.2",
    "reactotron-react-native": "^5.1.9",
    "reactotron-redux": "^3.1.10",
    "reactotron-redux-saga": "^4.2.3",
    "redux-saga": "^1.3.0",
    "tailwindcss": "^3.4.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.2",
    "@react-native/eslint-config": "0.75.2",
    "@react-native/metro-config": "0.75.2",
    "@react-native/typescript-config": "0.75.2",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-ft-flow": "^3.0.11",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.6.3",
    "prettier": "^3.0.0",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
