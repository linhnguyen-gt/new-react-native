{
    "name": "NewReactNative",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "yarn check:env && npx expo run:android --variant devDebug --device",
        "android:stg": "yarn check:env && APP_ENV=staging && npx expo run:android --variant stagingDebug --app-id com.newreactnative.stg --device",
        "android:pro": "yarn check:env && APP_ENV=production && npx expo run:android --variant productionDebug --app-id com.newreactnative.production --device",
        "ios": "yarn check:env && npx expo run:ios --device",
        "ios:stg": "yarn check:env && APP_ENV=staging && npx expo run:ios --scheme Staging --configuration Staging.Debug --device",
        "ios:prod": "yarn check:env && APP_ENV=production && npx expo run:ios --scheme Product --configuration Product.Debug --device",
        "check:env": "node scripts/check-env.js",
        "lint": "eslint .",
        "start": "npx expo start",
        "lint:fix": "eslint . --fix --ext .ts,.tsx,.js",
        "test": "jest",
        "clear:cache": "rm -rf node_modules && yarn cache clean && yarn && watchman watch-del-all && del %localappdata%Temphaste-map-* && del %localappdata%Tempmetro-cache && npx expo start --clear",
        "clear:expo": "npx expo start --clear",
        "env:push": "dotenv-vault push && dotenv-vault push staging && dotenv-vault push production",
        "env:pull": "dotenv-vault pull && dotenv-vault pull staging && dotenv-vault pull production",
        "env:setup": "node scripts/setup-env.js",
        "adb:reactotron": "adb reverse tcp:9090 tcp:9090",
        "update-versions": "node scripts/update-readme-versions.js",
        "postinstall": "lefthook install && yarn update-versions",
        "doctor": "npx expo-doctor"
    },
    "dependencies": {
        "@gluestack-style/react": "^1.0.57",
        "@gluestack-ui/config": "^1.1.20",
        "@gluestack-ui/image": "^0.1.17",
        "@gluestack-ui/nativewind-utils": "1.0.28",
        "@gluestack-ui/themed": "^1.1.73",
        "@hookform/resolvers": "^5.2.2",
        "@react-native-async-storage/async-storage": "2.2.0",
        "@react-native-vector-icons/ant-design": "^12.3.0",
        "@react-native-vector-icons/common": "^12.3.0",
        "@react-native-vector-icons/entypo": "^12.3.0",
        "@react-native-vector-icons/evil-icons": "^12.3.0",
        "@react-native-vector-icons/feather": "^12.3.0",
        "@react-native-vector-icons/ionicons": "^12.3.0",
        "@react-native-vector-icons/material-icons": "^12.3.0",
        "@react-native/new-app-screen": "0.81.1",
        "@react-navigation/native": "^7.1.17",
        "@react-navigation/stack": "^7.4.8",
        "@reduxjs/toolkit": "^2.9.0",
        "@shopify/flash-list": "2.0.2",
        "axios": "1.12.2",
        "dotenv": "^17.2.2",
        "expo": "~54.0.7",
        "lodash": "^4.17.21",
        "nativewind": "^4.2.1",
        "react": "19.1.0",
        "react-hook-form": "^7.62.0",
        "react-native": "0.81.4",
        "react-native-gesture-handler": "~2.28.0",
        "react-native-reanimated": "~4.1.0",
        "react-native-safe-area-context": "~5.6.0",
        "react-native-screens": "~4.16.0",
        "react-native-svg": "15.13.0",
        "react-native-worklets": "^0.5.1",
        "react-redux": "^9.2.0",
        "reactotron-react-native": "^5.1.16",
        "reactotron-redux": "^3.2.0",
        "reactotron-redux-saga": "^4.2.3",
        "redux-saga": "^1.3.0",
        "tailwindcss": "^3.4.17",
        "zod": "^4.1.8"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/preset-env": "^7.25.3",
        "@babel/runtime": "^7.25.0",
        "@commitlint/cli": "^19.6.1",
        "@commitlint/config-conventional": "^19.6.0",
        "@react-native-community/cli": "18.0.0",
        "@react-native-community/cli-platform-android": "18.0.0",
        "@react-native-community/cli-platform-ios": "18.0.0",
        "@react-native/babel-preset": "0.79.5",
        "@react-native/eslint-config": "0.79.5",
        "@react-native/metro-config": "0.79.5",
        "@react-native/typescript-config": "0.79.5",
        "@testing-library/jest-native": "^5.4.3",
        "@testing-library/react-native": "^12.9.0",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.16",
        "@types/react": "~19.1.10",
        "@types/react-dom": "~19.1.7",
        "@types/react-native-vector-icons": "^6.4.18",
        "@types/react-test-renderer": "^19.0.0",
        "@typescript-eslint/eslint-plugin": "^8.12.0",
        "@typescript-eslint/parser": "^8.12.0",
        "babel-plugin-module-resolver": "^5.0.2",
        "dotenv-vault": "^1.26.2",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-detox": "^1.0.0",
        "eslint-plugin-eslint-comments": "^3",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jest": "^27",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-native": "^4",
        "jest": "^29.7.0",
        "lefthook": "^1.11.12",
        "lint-staged": "^15.5.1",
        "prettier": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "react-dom": "19.1.0",
        "react-test-renderer": "19.0.0",
        "typescript": "~5.9.2"
    },
    "lint-staged": {
        "*.{js,ts,jsx,tsx}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "engines": {
        "node": ">=18"
    }
}
